/*
 

 
 */
import java.util.Scanner;
public class Exercicio02 {

	static int col = 0;
	public static void main(String[] args) {
		
		int lin = 5;		
		col = lerNumero();
		
		
		
		for (int l = 0; l < lin; l++){			
			for (int c = 0; c < col; c++){
				System.out.print("*");
			}
			System.out.println();
		}//--------------------------------------------

	}//------------------------------------------------

	static int lerNumero(){
		int c = 0;
		Scanner teclado = new Scanner(System.in);
		do{
			
			System.out.print("Digite o número de colunas: ");
			c = teclado.nextInt();
			if (c < 3 || c > 60){
				System.out.println("Valor fora do intervalo\n");
			}
		}while (c < 3 || c > 60);
		return c;
		//return teclado.nextInt(); 
	}
}//----------------------------------------------------
