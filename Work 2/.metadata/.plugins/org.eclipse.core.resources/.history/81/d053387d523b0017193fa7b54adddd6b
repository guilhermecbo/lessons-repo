
public class ExArray01 {

	public static void main(String[] args) {
		Pessoa pessoas[] = new Pessoa[20];
		
		Pessoa cliente = new Pessoa();
		
		pessoa[0] = new Pessoa();
		pessoa[0].setNome("Pedro");
		pessoa[0].setIdade(22);
		pessoa[0].setSalario(2000);
		
		System.out.println(cliente.getSalario()
				
				
				);

	}

}
