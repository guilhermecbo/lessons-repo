
public class Pessoa {
	private String nome;
	private int idade;
	private int salario;
	
	public void setNome(){
		this.nome = nome;
	}
	
	public String getNome(){
		return nome;
	}
	
	public void setIdade(){
		this.idade = idade;
	}
	
	public int getIdade(){
		return idade;
	}
	
	public void setSalario(){
		this.salario = salario;
	}
	
	public int getSalario(){
		return salario;
	}

}
